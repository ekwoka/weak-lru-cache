{
  "name": "@ekwoka/weak-lru-cache",
  "version": "0.0.1",
  "description": "A simple weak referenced Least Recently Used cache",
  "author": {
    "name": "Eric Kwoka",
    "email": "ninjatheory@gmail.com",
    "url": "https://thekwoka.net/"
  },
  "repository": "github:ekwoka/weak-lru-cache",
  "license": "ISC",
  "keywords": [
    "weakmap",
    "weakref",
    "cache",
    "lru",
    "least recently used"
  ],
  "type": "module",
  "private": true,
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "coverage": "vitest run --coverage",
    "lint": "eslint --fix ./src; prettier --write ./src --loglevel error",
    "lint:check": "eslint --max-warnings 10 ./src && prettier --check ./src",
    "prepare": "husky install",
    "size": "node scripts/esbuild.js",
    "test": "vitest"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "@vitest/coverage-c8": "^0.28.4",
    "esbuild": "^0.17.5",
    "eslint": "^8.33.0",
    "gzip-size": "^7.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "pretty-bytes": "^6.1.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vite-plugin-dts": "^1.7.2",
    "vitest": "^0.28.4"
  },
  "prettier": {
    "singleQuote": true,
    "bracketSameLine": true
  },
  "lint-staged": {
    "*.{js,ts,mjs}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,mdx,html,css,scss,less,graphql,yml,yaml}": [
      "prettier --write"
    ]
  }
}
